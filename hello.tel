-- File for small illustrative telomare programs and for testing

-- Simplest Hello Wold program.
main = \input -> ("Hello, World!", 0)


-- Pattern matching on Pair and Zero
foo = \case
  Pair a b -> (b, 0)
  Zero     -> ("Hello, world!", 0)

bar = \input -> case input of
                  Pair a b -> (b, 0)
                  Zero     -> ("Hello, world!", 0)
